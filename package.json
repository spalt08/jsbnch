{
  "name": "jsbnch",
  "version": "1.0.0",
  "description": "JS benchmark",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js --quiet --fix",
    "start": "webpack-dev-server --open",
    "start:storybook": "start-storybook -p 9096",
    "fetch:swagger": "yarn swagger-typescript-api --templates swagger-template -p https://spalt08.github.io/touchstone-api/swagger.json -o src/helpers -n api.ts && yarn eslint src/helpers/api.ts --quiet --fix",
    "build": "rimraf dist && webpack --mode=production --progress",
    "build:storybook": "build-storybook -o dist/storybook",
    "predeploy": "npm run build && npm run build:storybook",
    "deploy": "gh-pages -d dist",
    "analyze": "npm run build -- --analyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spalt08/jsbnch.git"
  },
  "author": "Konstantin Darutkin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/spalt08/jsbnch/issues"
  },
  "homepage": "https://spalt08.github.io/jsbnch",
  "dependencies": {
    "clsx": "^1.1.1",
    "monaco-editor": "^0.22.3",
    "prismjs": "^1.22.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.9.2",
    "react-query": "^2.23.1",
    "react-router-dom": "^5.2.0",
    "storybook": "^6.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@storybook/react": "^6.1.11",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "autoprefixer": "^10.0.1",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.6",
    "monaco-editor-webpack-plugin": "^3.0.0",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.4",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.1",
    "serviceworker-webpack-plugin": "^1.0.1",
    "swagger-typescript-api": "^4.0.4",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
